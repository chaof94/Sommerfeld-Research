journal = {Physical Review B - Condensed Matter and Materials Physics},
month = {feb},
number = {8},
pages = {085419},
pmid = {21646276},
title = {{Dyadic Green's functions of thin films: Applications within plasmonic solar cells}},
url = {http://link.aps.org/doi/10.1103/PhysRevB.83.085419},
volume = {83},
year = {2011}
}
