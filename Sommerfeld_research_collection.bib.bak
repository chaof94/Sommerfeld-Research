journal = {IEEE Transactions on Microwave Theory and Techniques},
keywords = {CMOS,direct terahertz detection,distributed resistive self-mixing,patch antennas,plasmonic detection,submillimeter-wave detectors,terahertz detectors},
number = {12},
pages = {3834--3843},
title = {{CMOS integrated antenna-coupled field-effect transistors for the detection of radiation from 0.2 to 4.3 THz}},
