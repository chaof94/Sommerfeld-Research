isbn = {1076201300477},
issn = {18666906},
journal = {Journal of Infrared, Millimeter, and Terahertz Waves},
keywords = {Heterodyne detection,Silicon CMOS circuits,Terahertz emitters and detectors,Terahertz imaging},
number = {1},
pages = {63--80},
title = {{Exploration of terahertz imaging with silicon MOSFETs}},
volume = {35},
year = {2014}
}
